// stage=ir, args=-C dump=ir -C ir-dump-mode=pretty

// @@Temporary: disabled before new tc becomes default

main := () => {
    mut t := 0;

    loop {
        t += 1;
        if t > 10 {
            break;
        }

        mut m := t + 1;

        loop {
            match m {
                0..3 => {
                    continue;
                },
                4 => {
                    break;
                },
                _ => {
                    m - 1;
                }
            }
        }
    }
}
