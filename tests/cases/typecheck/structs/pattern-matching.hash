// stage=typecheck
Dog := struct(
    age: i32,
    foo: i32,
    // width: f32,
    // height: f32,
    gender: char
);

main := () -> i32 => {
  viktor := Dog(age = 2, foo = 3, gender = 'n');

  item := match viktor {
    Dog(age = 15, gender, foo) => {
      age
    };
    _ => { 2 };
  };

  item
};
